          <tl:Interval>
            <tl:start rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-02-17T14:15:00Z</tl:start>
            <tl:end rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-02-17T16:15:00Z</tl:end>
          </tl:Interval>
        </event:time>
